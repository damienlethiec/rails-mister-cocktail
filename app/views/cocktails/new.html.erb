<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-8 col-xs-offset-2">
        <%= simple_form_for(@cocktail) do |f| %>
          <%= f.input :name %>
          <%= f.input :photo, as: :attachinary %>
          <%= f.button :submit, class: "btn btn-danger"%>
          <%= link_to "Back to cocktail list", cocktails_path, class: "btn btn-default" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
